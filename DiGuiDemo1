public class DiGuiDemo1{
  public static void main(String[] args){
    syso(20);
    

  }
  
      //create digui method
    public static int diGui(int n){
      if(n == 1 || n ==2){
        return 1;
      }else {
        return diGui(n)+digui(n-1);
      }
    }
}
