/*
1,1,2,3,5,8,13....... 

20

*/

public class DiGuiDemo1{
  public static void main(String[] args){
    syso(f(20));
    

  }
  
      //create digui method
    public static int diGui(int n){
      if(n == 1 || n == 2){
        return 1;
      }else {
        return diGui(n-1)+digui(n-2);
      }
    }
}
